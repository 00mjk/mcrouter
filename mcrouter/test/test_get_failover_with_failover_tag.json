{
  "clusters": {
    "A": {
      "pools": {
        "A.gut": {
          "servers": [ "localhost:12345" ]
        },
        "A.wildcard": {
          "servers": [ "localhost:12346" ]
        }
      }
    }
  },
  "routes" : [
    {
      "aliases" : [ "/a/a/" ],
      "route" : {
        "type" : "FailoverWithExptimeRoute",
        "failover" : "PoolRoute|A.gut",
        "normal" : "PoolRoute|A.wildcard",
        "failover_exptime" : 3,
        "settings": {
          "failover_tag": true
        }
      }
    }
  ]
}