{
  "clusters": {
    "A": {
      "pools": {
        "A.wildcard": {
          "servers": [ "localhost:12345" ]
        }
      }
    },
    "B": {
      "pools": {
        "B.wildcard": {
          "servers": [ "localhost:12346" ]
        }
      }
    }
  },
  "routes": [
    {
      "aliases": [ "/a/a/" ],
      "route": "PoolRoute|A.wildcard"
    },
    {
      "aliases": [ "/b/b/" ],
      "route": "PoolRoute|B.wildcard"
    }
  ]
}